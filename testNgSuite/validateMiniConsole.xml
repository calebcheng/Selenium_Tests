<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >


<suite name="Mini Console test suites">

	<parameter name="suiteEnv" value="ENVIRONMENT_PROPERTIES_FOLDER" />


	<test name="Validate assets">

