<?xml version="1.0" encoding="UTF-8"?>


<suite name="NewSuiteTest">

  <listeners>
	<listener class-name="com.ibm.mega.test.framework.TestNGReportGenerator" />
	<listener class-name="com.ibm.mega.test.framework.TestCaseListener" />
	<listener class-name="com.ibm.mega.test.framework.TestMethodRetryListener" />
  </listeners>

<!-- Replace ENVIRONMENT_PROPERTIES_FOLDER with the environment properties folder corresponding to  the 
	environment to be used for the test execution -->
	
  <parameter name="suiteEnv" value="m3_regression" />
  <parameter name="suiteDesc" value="10.62 m3 regression"/>
  
