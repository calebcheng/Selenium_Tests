<?xml version="1.0" encoding="UTF-8"?>

  
<suite name="MEG_Reg_UntrustedCertificateHandling">

  <listeners>
	<listener class-name="com.ibm.mega.test.framework.TestNGReportGenerator" />
	<listener class-name="com.ibm.mega.test.framework.TestCaseListener" />
	<listener class-name="com.ibm.mega.test.framework.TestMethodRetryListener" />
  </listeners>

  <parameter name="suiteEnv" value="M3" />
  <parameter name="suiteDesc" value="Regression test MEG with different Untrusted Certificate Mode Handling"/>
  <parameter name="uploadReport" value="false" />
  <parameter name="confirmBeforeUpload" value="false" />
 
  <test name="Accept All" >
    <classes>
		<class name="com.ibm.mega.test.gateway.devTesting.ConfigAcceptAll" />
		<class name="com.ibm.mega.test.device.STO_66200_BadSSLExpired_ACCEPT" />
		<class name="com.ibm.mega.test.device.STO_66207_BadSSLWrongHost_ACCEPT" />
		<class name="com.ibm.mega.test.device.STO_66205_BadSSLUntrusted_ACCEPT" />
    </classes>
  </test> 
  
  <test name="Reject All">
  	<classes>
		<class name="com.ibm.mega.test.gateway.devTesting.ConfigRejectAll" />
		<class name="com.ibm.mega.test.device.STO_66201_BadSSLExpired_REJECT" />
		<class name="com.ibm.mega.test.device.STO_66208_BadSSLWrongHost_REJECT" />
		<class name="com.ibm.mega.test.device.STO_66204_BadSSLUntrusted_REJECT" />
    </classes>
  </test>
  
  <test name="Prompt User" >
    <classes>
		<class name="com.ibm.mega.test.gateway.devTesting.ConfigPromptUser" />
		<class name="com.ibm.mega.test.device.STO_66202_BadSSLExpired_PROMPT" />
		<class name="com.ibm.mega.test.device.STO_66209_BadSSLWrongHost_PROMPT" />
		<class name="com.ibm.mega.test.device.STO_66203_BadSSLUntrusted_PROMPT" />
    </classes>
  </test> 

</suite>  



